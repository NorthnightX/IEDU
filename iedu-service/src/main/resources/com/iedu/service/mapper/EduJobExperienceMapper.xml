<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iedu.service.mapper.EduJobExperienceMapper">
    
    <resultMap type="EduJobExperience" id="EduJobExperienceResult">
        <result property="eduId"    column="edu_id"    />
        <result property="eduExp"    column="edu_exp"    />
        <result property="eduCreateUser"    column="edu_create_user"    />
        <result property="eduModifyUser"    column="edu_modify_user"    />
        <result property="eduCreateTime"    column="edu_create_time"    />
        <result property="eduModifyTime"    column="edu_modify_time"    />
    </resultMap>

    <sql id="selectEduJobExperienceVo">
        select edu_id, edu_exp, edu_create_user, edu_modify_user, edu_create_time, edu_modify_time from edu_job_experience
    </sql>

    <select id="selectEduJobExperienceList" parameterType="EduJobExperience" resultMap="EduJobExperienceResult">
        <include refid="selectEduJobExperienceVo"/>
        <where>  
            <if test="eduExp != null  and eduExp != ''"> and edu_exp = #{eduExp}</if>
            <if test="eduCreateUser != null "> and edu_create_user = #{eduCreateUser}</if>
            <if test="eduModifyUser != null "> and edu_modify_user = #{eduModifyUser}</if>
            <if test="eduCreateTime != null "> and edu_create_time = #{eduCreateTime}</if>
            <if test="eduModifyTime != null "> and edu_modify_time = #{eduModifyTime}</if>
        </where>
    </select>
    
    <select id="selectEduJobExperienceByEduId" parameterType="Long" resultMap="EduJobExperienceResult">
        <include refid="selectEduJobExperienceVo"/>
        where edu_id = #{eduId}
    </select>
    <select id="selectAll"  parameterType="EduJobExperience" resultMap="EduJobExperienceResult">
        select edu_id, edu_exp from edu_job_experience
    </select>

    <insert id="insertEduJobExperience" parameterType="EduJobExperience" useGeneratedKeys="true" keyProperty="eduId">
        insert into edu_job_experience
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="eduExp != null">edu_exp,</if>
            <if test="eduCreateUser != null">edu_create_user,</if>
            <if test="eduModifyUser != null">edu_modify_user,</if>
            <if test="eduCreateTime != null">edu_create_time,</if>
            <if test="eduModifyTime != null">edu_modify_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="eduExp != null">#{eduExp},</if>
            <if test="eduCreateUser != null">#{eduCreateUser},</if>
            <if test="eduModifyUser != null">#{eduModifyUser},</if>
            <if test="eduCreateTime != null">#{eduCreateTime},</if>
            <if test="eduModifyTime != null">#{eduModifyTime},</if>
         </trim>
    </insert>

    <update id="updateEduJobExperience" parameterType="EduJobExperience">
        update edu_job_experience
        <trim prefix="SET" suffixOverrides=",">
            <if test="eduExp != null">edu_exp = #{eduExp},</if>
            <if test="eduCreateUser != null">edu_create_user = #{eduCreateUser},</if>
            <if test="eduModifyUser != null">edu_modify_user = #{eduModifyUser},</if>
            <if test="eduCreateTime != null">edu_create_time = #{eduCreateTime},</if>
            <if test="eduModifyTime != null">edu_modify_time = #{eduModifyTime},</if>
        </trim>
        where edu_id = #{eduId}
    </update>

    <delete id="deleteEduJobExperienceByEduId" parameterType="Long">
        delete from edu_job_experience where edu_id = #{eduId}
    </delete>

    <delete id="deleteEduJobExperienceByEduIds" parameterType="String">
        delete from edu_job_experience where edu_id in 
        <foreach item="eduId" collection="array" open="(" separator="," close=")">
            #{eduId}
        </foreach>
    </delete>
</mapper>