<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iedu.service.mapper.EduCompanyMapper">
    
    <resultMap type="EduCompany" id="EduCompanyResult">
        <result property="eduId"    column="edu_id"    />
        <result property="eduCompanyName"    column="edu_company_name"    />
        <result property="eduCompanyScaleId"    column="edu_company_scale_id"    />
        <result property="eduCompanyIndustryId"    column="edu_company_industry_id"    />
        <result property="eduFinancingStageId"    column="edu_financing_stage_id"    />
        <result property="eduCompanyIntroduce"    column="edu_company_introduce"    />
        <result property="eduInformationId"    column="edu_information_id"    />
        <result property="eduAddressId"    column="edu_address_id"    />
        <result property="eduCreateUser"    column="edu_create_user"    />
        <result property="eduModifyUser"    column="edu_modify_user"    />
        <result property="eduCreateTime"    column="edu_create_time"    />
        <result property="eduModifyTime"    column="edu_modify_time"    />
    </resultMap>

    <sql id="selectEduCompanyVo">
        select edu_id, edu_company_name, edu_company_scale_id, edu_company_industry_id, edu_financing_stage_id, edu_company_introduce, edu_information_id, edu_address_id, edu_create_user, edu_modify_user, edu_create_time, edu_modify_time from edu_company
    </sql>

    <select id="selectEduCompanyList" parameterType="EduCompany" resultMap="EduCompanyResult">
        <include refid="selectEduCompanyVo"/>
        <where>  
            <if test="eduCompanyName != null  and eduCompanyName != ''"> and edu_company_name like concat('%', #{eduCompanyName}, '%')</if>
            <if test="eduCompanyScaleId != null "> and edu_company_scale_id = #{eduCompanyScaleId}</if>
            <if test="eduCompanyIndustryId != null "> and edu_company_industry_id = #{eduCompanyIndustryId}</if>
            <if test="eduFinancingStageId != null "> and edu_financing_stage_id = #{eduFinancingStageId}</if>
            <if test="eduCompanyIntroduce != null  and eduCompanyIntroduce != ''"> and edu_company_introduce = #{eduCompanyIntroduce}</if>
            <if test="eduInformationId != null "> and edu_information_id = #{eduInformationId}</if>
            <if test="eduAddressId != null "> and edu_address_id = #{eduAddressId}</if>
            <if test="eduCreateUser != null "> and edu_create_user = #{eduCreateUser}</if>
            <if test="eduModifyUser != null "> and edu_modify_user = #{eduModifyUser}</if>
            <if test="eduCreateTime != null "> and edu_create_time = #{eduCreateTime}</if>
            <if test="eduModifyTime != null "> and edu_modify_time = #{eduModifyTime}</if>
        </where>
    </select>
    
    <select id="selectEduCompanyByEduId" parameterType="Long" resultMap="EduCompanyResult">
        <include refid="selectEduCompanyVo"/>
        where edu_id = #{eduId}
    </select>
    <select id="selectAllCompany" parameterType="EduCompany" resultMap="EduCompanyResult">
        select edu_id, edu_company_name from edu_company
    </select>

    <insert id="insertEduCompany" parameterType="EduCompany" useGeneratedKeys="true" keyProperty="eduId">
        insert into edu_company
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="eduCompanyName != null">edu_company_name,</if>
            <if test="eduCompanyScaleId != null">edu_company_scale_id,</if>
            <if test="eduCompanyIndustryId != null">edu_company_industry_id,</if>
            <if test="eduFinancingStageId != null">edu_financing_stage_id,</if>
            <if test="eduCompanyIntroduce != null">edu_company_introduce,</if>
            <if test="eduInformationId != null">edu_information_id,</if>
            <if test="eduAddressId != null">edu_address_id,</if>
            <if test="eduCreateUser != null">edu_create_user,</if>
            <if test="eduModifyUser != null">edu_modify_user,</if>
            <if test="eduCreateTime != null">edu_create_time,</if>
            <if test="eduModifyTime != null">edu_modify_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="eduCompanyName != null">#{eduCompanyName},</if>
            <if test="eduCompanyScaleId != null">#{eduCompanyScaleId},</if>
            <if test="eduCompanyIndustryId != null">#{eduCompanyIndustryId},</if>
            <if test="eduFinancingStageId != null">#{eduFinancingStageId},</if>
            <if test="eduCompanyIntroduce != null">#{eduCompanyIntroduce},</if>
            <if test="eduInformationId != null">#{eduInformationId},</if>
            <if test="eduAddressId != null">#{eduAddressId},</if>
            <if test="eduCreateUser != null">#{eduCreateUser},</if>
            <if test="eduModifyUser != null">#{eduModifyUser},</if>
            <if test="eduCreateTime != null">#{eduCreateTime},</if>
            <if test="eduModifyTime != null">#{eduModifyTime},</if>
         </trim>
    </insert>

    <update id="updateEduCompany" parameterType="EduCompany">
        update edu_company
        <trim prefix="SET" suffixOverrides=",">
            <if test="eduCompanyName != null">edu_company_name = #{eduCompanyName},</if>
            <if test="eduCompanyScaleId != null">edu_company_scale_id = #{eduCompanyScaleId},</if>
            <if test="eduCompanyIndustryId != null">edu_company_industry_id = #{eduCompanyIndustryId},</if>
            <if test="eduFinancingStageId != null">edu_financing_stage_id = #{eduFinancingStageId},</if>
            <if test="eduCompanyIntroduce != null">edu_company_introduce = #{eduCompanyIntroduce},</if>
            <if test="eduInformationId != null">edu_information_id = #{eduInformationId},</if>
            <if test="eduAddressId != null">edu_address_id = #{eduAddressId},</if>
            <if test="eduCreateUser != null">edu_create_user = #{eduCreateUser},</if>
            <if test="eduModifyUser != null">edu_modify_user = #{eduModifyUser},</if>
            <if test="eduCreateTime != null">edu_create_time = #{eduCreateTime},</if>
            <if test="eduModifyTime != null">edu_modify_time = #{eduModifyTime},</if>
        </trim>
        where edu_id = #{eduId}
    </update>

    <delete id="deleteEduCompanyByEduId" parameterType="Long">
        delete from edu_company where edu_id = #{eduId}
    </delete>

    <delete id="deleteEduCompanyByEduIds" parameterType="String">
        delete from edu_company where edu_id in 
        <foreach item="eduId" collection="array" open="(" separator="," close=")">
            #{eduId}
        </foreach>
    </delete>
</mapper>